{
  "earlyboundtypes": [
    {
      /*
            Comma seperate list of entity logical names.
            I've not provided support for -all- entities because this results in unneccessarily large plugins!
            */
      "entities": "account,contact,ccllc_alternatebranch,ccllc_processauthorizedchannel,ccllc_processstep,ccllc_processstepauthorizedchannel,ccllc_processsteprequirement,ccllc_processsteptype,ccllc_requirementwaiverrole,ccllc_stephistory,ccllc_transaction,ccllc_transactiongroup,ccllc_transactioninitialfee,ccllc_transactionprocess,ccllc_transactionrequirement,ccllc_transactionrequirementwaiverrole,ccllc_transactiontype,ccllc_transactiontypeauthorizedchannel,ccllc_transactiontypeauthorizedrole,ccllc_transactiontypecontext,ccllc_transactiondeficiency",
      /*            
            Set to 'true' to generate Enums for optionsets
            */
      "generateOptionsetEnums": "true",
      /*
            Set to 'true' to generate Enums for States and Statuses
            */
      "generateStateEnums": "true",
      /*
            Set to 'true' to generate Enums for Global optionsets
            */
      "generateGlobalOptionsets": false,
      /*
            The path (relative to this file) to output
            */
      "filename": "EarlyBoundTypes.cs",
      /*
            Output one file per class/optionset/service/request
            */
      "oneTypePerFile": true,
      /*
            The namespace to put the classes under
            */
      "classNamespace": "CCLLC.BTF.Process.CDS"
     
    }
  ]
}